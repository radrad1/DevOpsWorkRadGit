{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "webAppName": {
      "type": "string",
      "minLength": 1
    },
    "slotName": {
      "type": "string",
      "minLength": 1
    },
    "environment": {
      "type": "string",
      "minLength": 1
    },
    "releaseDefinitionReleaseEnvironment": {
      "type": "string",
      "minLength": 1
    },
    "sqlServerName": {
      "type": "string",
      "minLength": 1
    },
    "databaseName": {
      "type": "string",
      "minLength": 1
    },
    "administratorLogin": {
      "type": "string",
      "minLength": 1
    },
    "administratorLoginPassword": {
      "type": "securestring",
      "minLength": 1
    },
    "webAppSettingsUse32BitWorkerProcess": {
      "type": "bool",
      "defaultValue": false
    },
    "webAppSettingsAlwaysOn": {
      "type": "bool",
      "defaultValue": false
    },
    "webAppSettingsRemoteDebuggingEnabled": {
      "type": "bool",
      "defaultValue": false
    }
  },
  "variables": {
  },
  "resources": [
  ],
  "outputs": {
  }
}